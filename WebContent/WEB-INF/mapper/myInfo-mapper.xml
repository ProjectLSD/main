<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="myInfo">
<select id="infoMember" parameterType="java.lang.String" resultType="java.util.HashMap">
		select * from member where id = #{id}
</select>

<select id="infoBand" parameterType="java.lang.String" resultType="java.util.HashMap">
		select * from band where id = #{id}
</select>

<update id="updateMember" parameterType="java.util.HashMap"> 
		update member set pass=#{pass}, email=#{email}, phonenum1=#{phonenum1},phonenum2=#{phonenum2},phonenum3=#{phonenum3},name=#{name} where id=#{id}
	</update>
</mapper>